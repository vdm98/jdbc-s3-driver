{
  "label": "depts",
  "location": "s3://222/depts.csv",
  "format": {
    "delimiter": ",",
    "lineSeparator": "\n",
    "quoteChar": "\"",
    "escape": "\\",
    "commentChar": "#",
    "header": false,
    "strictQuotes": false,
    "ignoreLeadingWhiteSpace": false,
    "ignoreQuotations": false,
    "nullFieldIndicator": "neither",
    "compression": "none",
    "datePattern": "dd.MM.yyyy",
    "timePattern": "HH:mm:ss",
    "datetimePattern": "dd.MM.yyyy HH:mm:ss",
    "timestampPattern": "yyyy-MM-dd'T'HH:mm:ss.SSSZ"
  },
  "columns": [
    {
      "label": "id",
      "datatype": "integer",
      "nullable": false
    },
    {
      "label": "deptname",
      "datatype": "string",
      "nullable": false
    },
    {
      "label": "address",
      "datatype": "string",
      "nullable": false
    },
    {
      "label": "opendate",
      "datatype": "date",
      "nullable": false
    }
  ]
}