  "columns": [
    {
      "label": "id",
      "datatype": "integer",
      "nullable": false
    },
    {
      "label": "firstname",
      "datatype": "string",
      "nullable": false
    },
    {
      "label": "lastname",
      "datatype": "string",
      "nullable": false
    }
  ]
